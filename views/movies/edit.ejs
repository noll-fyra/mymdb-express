<h1>Form to edit existing movie
    <%- theMovie.id %></h1>

<form action="/movies/<%- theMovie.id%>" method="POST">
    <input type="text" name="name" value="<%- theMovie.name %>">
    <input type="text" name="synopsis" value="<%- theMovie.synopsis %>">
    <input type="text" name="rating" value="<%- theMovie.rating %>">
    <button type="submit">Submit Changes</button>
</form>
